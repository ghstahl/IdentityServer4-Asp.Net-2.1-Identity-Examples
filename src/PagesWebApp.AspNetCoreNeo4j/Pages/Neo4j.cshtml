@page
@model PagesWebApp.AspNetCoreNeo4j.Pages.Neo4jModel
@{
    ViewData["Title"] = "Neo4j";
}

<h2>@ViewData["Title"]</h2>
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <button type="submit" class="btn btn-primary" name="button" value="populate" CausesValidation="false" autofocus>Initialize IdentityServer4 Data</button>
            <button type="submit" class="btn btn-default" name="button" value="dropDatabase" CausesValidation="false">Drop Database</button>
        </form>
    </div>
</div>